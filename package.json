{
  "name": "react-initial-setup",
  "private": true,
  "version": "0.0.1",
  "description": "React Initial Setup",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "yarn test:unit",
    "test:unit": "jest --config='config/jest/index.json'",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:ci": "yarn start-server-and-test start:e2e http://localhost:8080 test:e2e",
    "test:all": "yarn lint && yarn test:unit && yarn test:e2e:ci",
    "start:e2e": "cross-env NODE_ENV=development webpack-dev-server --progress --hot --inline --mode development --config=webpack/e2e.js",
    "start": "yarn lint && cross-env PORT=${PORT} NODE_ENV=development webpack-dev-server --progress --hot --inline --mode development --config=webpack/development.js",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --mode production --config=webpack/production.js",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty src *.js",
    "production:deploy": "",
    "staging:deploy": ""
  },
  "author": "juliobetta",
  "browserslist": [
    "last 2 versions",
    "not ie <= 8"
  ],
